{
  "hosting": {
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/foodorderapp/**",
        "destination": "/index.html"
      }
    ],
    "appAssociation": "AUTO"
  }
}
